{
  "name": "formless-tools",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest ./lib",
    "test-coverage": "jest --coverage ./lib",
    "publish": "tsc && jest ./lib && npm publish"
  },
  "author": "Liang Rizhong",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/testing": "^8.4.7",
    "archiver": "^5.3.1",
    "express": "^4.18.1",
    "g": "^2.0.1",
    "jest": "^28.1.1",
    "node-xlsx": "^0.21.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "roots": [
      "<rootDir>/lib/"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "tsconfig-paths": "^4.0.0"
  }
}
